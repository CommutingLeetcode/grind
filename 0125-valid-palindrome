class Solution:
    def isPalindrome(self, s: str) -> bool:
        l = 0
        r = len(s) - 1
        while (l <= r):
            #store the letters first for easy access
            left = s[l]
            right = s[r]
            if(not left.isalnum()):
                l += 1
                continue
            if(not right.isalnum()):
                r -= 1
                continue
            if (left.lower() != right.lower()):
                return False
            l += 1
            r -= 1
        return True
    
'''
time complexity: O(n/2) -> O(n)
space complexity: O(1) we're not really creating any arrays
'''
